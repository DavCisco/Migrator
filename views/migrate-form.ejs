<% include header.ejs %>

	<div id='titleTable'>
		<h2>Devices</h2>
	</div>
	
	<main id='main'>

	<br>
	
		<form>
			<fieldset>
				<legend><b>On-premises endpoint migration</b></legend>
					<p><i>Select the .csv file:</i></p>
					<p><input type='file' id='fileselect' accept='text/plain' value='fileselect'/></p>
					<p><i>Credentials for the on-prem endpoints:</i></p>
					<p><label>username: </label><input type='text' id='username' class='input' placeholder='admin'/></p>
					<p><label>password: </label><input type='password' id='pwd'/ class='input'></p>
					<p><i>Security:</i></p>
					<p><input id='disableAdmin' type='checkbox'/> Disable local admin user and macros</p>
			</fieldset>
		</form>
		<br>
		<button type='button' class='button' onclick='sendData()'>Migrate</button>
		<button type='button' class='button'><a href='/refresh'>Cancel</a></button>

	</main>


	<% include footer.ejs %>

</body>

